{
    "name": "vscode-plush",
    "displayName": "vscode-plush",
    "description": "Plush syntax support for VSCode",
    "version": "0.1.3",
    "publisher": "rubbersheep",
    "engines": {
        "vscode": "^1.13.0"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hasit/vscode-plush"
    },
    "homepage": "https://github.com/hasit/vscode-plush/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/hasit/vscode-plush/issues"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            }
        ],
        "languages": [
            {
                "id": "plush",
                "aliases": [
                    "Plush",
                    "plush",
                    "fizz"
                ],
                "extensions": [
                    ".plush",
                    ".fizz",
                    ".html"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "plush",
                "scopeName": "source.plush",
                "path": "./syntaxes/fizz.tmLanguage.json"
            },
            {
                "language": "plush",
                "scopeName": "text.html.plush",
                "path": "./syntaxes/html.tmLanguage.json",
                "embeddedLanguages": {
                    "text.html": "html",
                    "source.plush": "plush",
                    "source.sql": "sql",
                    "text.xml": "xml",
                    "source.js": "javascript",
                    "source.json": "json",
                    "source.css": "css"
                }
            }
        ]
    }
}
